{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./app"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": ["**/node_modules/**", "**/*.test.ts", "**/*.test.tsx"],
  "excludePrivate": true,
  "excludeProtected": true,
  "readme": "none",
  "plugin": [],
  "theme": "default",
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": ["API Routes", "Components", "*"],
  "sort": ["source-order"]
}
